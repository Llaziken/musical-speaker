{
  "name": "musical-speaker",
  "version": "1.0.1",
  "description": "Adds a speaker to the game that includes 189 instruments for use in music production.",
  "main": "src/control.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/factorio": "git+https://github.com/sguest/factorio-types.git",
    "lua-types": "^2.8.0",
    "typescript-to-lua": "^0.36.0"
  },
  "scripts": {
    "clean": "rm -r dist",
    "dev": "tstl --watch",
    "build": "tstl; cp -r resources/locale dist/; cp -r resources/script dist/",
    "copy-sounds": "cp -r resources/sound dist/",
    "dist": "clean; build; copy-sounds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xcelled/musical-speaker.git"
  },
  "keywords": [
    "factorio",
    "speaker",
    "music",
    "mod"
  ],
